<%- include("parts/html-head") %>
<%- include("parts/navbar") %>
<div class="container">
    <div class="row">
        <div class="col-6">
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th>Employee ID</th>
                        <th>Name</th>
                        <th>Age</th>
                    </tr>
                </thead>
                <tbody>
                    <% for(let s of sales) {  %>
                        <tr>
                            <td><%= s.id %></td>
                            <td><%= s.name %></td>
                            <td><%= s.age %></td>
                        </tr>
                    <% } %>
                </tbody>
            </table>
        </div>
    </div>
</div>
<%- include("parts/scripts") %>
<%- include("parts/html-foot") %>
